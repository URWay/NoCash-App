<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.luiz1.nocash.ContaFragment"
    android:background="@drawable/bgconta">


    <Button
        android:id="@+id/btnatendimento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignStart="@+id/btntransacoes"
        android:layout_below="@+id/btntransacoes"
        android:layout_marginEnd="14dp"
        android:layout_marginTop="14dp"
        android:layout_toStartOf="@+id/btncomprovante"
        android:padding="20dp"
        android:text="dúvidas" />

    <Button
        android:id="@+id/btncomprovante"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btncredito"
        android:layout_alignEnd="@+id/btntransacoes"
        android:padding="20dp"
        android:text="Comprovantes" />

    <Button
        android:id="@+id/btntransacoes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="88dp"
        android:padding="50dp"
        android:textSize="15dp"
        android:text="Histórico de transações"
        android:textAlignment="viewStart" />

    <Button
        android:id="@+id/btncredito"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/btntransacoes"
        android:layout_alignStart="@+id/btnatendimento"
        android:layout_below="@+id/btnatendimento"
        android:layout_marginTop="25dp"
        android:padding="50dp"
        android:text="Crédito"
        android:textAlignment="center"
        android:textSize="15dp" />




</RelativeLayout>
